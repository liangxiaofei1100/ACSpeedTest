    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="17" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name=".AndroidCommunicationSpeedTestActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="com.dreamlink.communication.action.app" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>

            <meta-data
                android:name="app_id"
                android:value="202" />
        </activity>
        <activity
            android:name=".SpeedTestClient"
            android:label="SpeedTestClient"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".SpeedTestServer"
            android:label="SpeedTestServer"
            android:screenOrientation="portrait" >
        </activity>
    </application>

</manifest>